import java.math.*;

class Solution {
    public String removeDigit(String number, char digit) {
    
        String max="0";

        for(int i=0;i<=number.length()-1;i++){
            if(number.charAt(i)==digit){
                String ans=number.substring(0,i)+number.substring(i+1);

                if(ans.compareTo(max)>0){
                    max=ans;
                }
            }
        }

        return max;
    }
}
