class Solution {
    public int removeElement(int[] nums, int val) {

        int ans = 0;
        for(int x : nums){
            if(x != val){
                ans++;
            }
        }

        for(int i=0; i<nums.length;i++){
            if(nums[i]==val){
                nums[i] = 101;
            }
        }
        Arrays.sort(nums);


        return ans;
        
    }
}
