class Solution {
    public int lengthOfLastWord(String s) {

        int ans = 0;

        String temp = s.trim();
        String space = " ";

        temp = space + temp;
        int len = temp.length();
 
        while(temp.charAt(len-1)!=' '){
            ans++;
            len--;
        }   
        return ans;  
    }
}
